#Region Property Getters and Setters
public Sub SetClasses(Classes As String) As VBtn
	If mElement <> Null Then
		mElement.AddClass(Classes)
	End If
	mClasses = Classes
	Return Me
End Sub

public Sub GetClasses() As String
	Return mClasses
End Sub

